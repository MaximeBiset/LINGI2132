package junit;

import pass.LeftShift;
import junit.framework.TestCase;


public class LeftShiftTest extends TestCase
{
	private LeftShift leftShift;
	
	protected void setUp()
	{
		super.setUp();
		leftShift = new LeftShift();
	}
	
	protected void tearDown()
	{
		super.tearDown();
	}
	
	public void testLeftShift()
	{	
		int x = 0x1000;
		this.assertEquals(leftShift.leftShift(0x0001), 0x0010);
		this.assertEquals(left.shift.leftShift(0x1000), 0x0001);
	}
}
