package junit;

import junit.framework.TestCase;
import pass.GreaterThanOrEqual;

public class GreaterThanOrEqualTest extends TestCase
{
	GreaterThanOrEqual GE;
	public void setUp()
	{
		super.setUp();
		GE = new GreaterThanOrEqual();
	}
	
	public void tearDown()
	{
		super.tearDown();
	}
	
	public void testGreaterThanOrEqual()
	{
		assertEquals(GE.GreaterThanOrEqual(42, 0), true);
		assertEquals(GE.GreaterThanOrEqual(42, 42), true);
		assertEquals(GE.GreaterThanOrEqual(0, 42), false);
	}
}
